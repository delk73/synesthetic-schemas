              "velocity": {
                "curve": "linear",
                "scale": 1.0,
                "source": "grid.pressure",
                "threshold": 0.02
              }
            }
          },
          {
            "channel": "hapticPulse",
            "op": "set",
            "target": "haptic.intensity",
            "value": {
              "scale": 1.0,
              "source": "grid.pressure"
            }
          },
          {
            "channel": "visualNudge",
            "op": "add",
            "target": "shader.u_initial_r",
            "value": {
              "scale": 0.1,
              "source": "grid.pressure"
            }
          }
        ],
        "execution": "client",
        "id": "grid_to_multimodal_mapping",
        "trigger": {
          "params": {
            "cooldown": 100,
            "gridSize": 8
          },
          "type": "grid_cell"
        }
      }
    ]
  },
  "shader": {
    "fragment_shader": "#define MAX_ITERATIONS 50\nprecision mediump float;uniform vec2 u_resolution;uniform float u_time;uniform float u_px;uniform float u_py;uniform float u_initial_r;uniform float u_poX;uniform float u_poY;uniform float u_ro;uniform float u_str;uniform float u_stro;uniform float u_m;uniform float u_mo;uniform float u_sub;uniform float u_subo;uniform int u_iterations;uniform float u_blendMode;uniform float u_offsetCurve;uniform float u_curveFrequency;uniform float u_curveAmplitude;float get_curve_value(float progress,float curve_mode,float freq){float pi=3.14159;float linear_val=progress;float sine_val=sin(progress*freq*2.0*pi);return mix(linear_val,sine_val,clamp(curve_mode,0.0,1.0));}void main(){vec2 st=gl_FragCoord.xy/u_resolution.xy;st=st*2.0-1.0;st.x*=u_resolution.x/u_resolution.y;vec2 p_initial=st-vec2(u_px,u_py);vec2 po=vec2(u_poX,u_poY);float tmp=1e+9;float denominator=max(1.0,float(u_iterations-1));for(int i=0;i<MAX_ITERATIONS;i++){if(i>=u_iterations)break;float progress=float(i)/denominator;float curve_mod=get_curve_value(progress,u_offsetCurve,u_curveFrequency)*u_curveAmplitude;vec2 p_current=p_initial+po*curve_mod;float r_current=u_initial_r+u_ro*curve_mod;float str_current=u_str+u_stro*curve_mod;float m_current=u_m+u_mo*curve_mod;float sub_current=u_sub+u_subo*curve_mod;float current_val;if(str_current<=0.0){current_val=(length(p_current)-r_current)*m_current-sub_current;}else{current_val=(abs(length(p_current)-r_current)-str_current)*m_current-sub_current;}if(i==0){tmp=current_val;}else{float min_blend=min(tmp,current_val);float sub_blend=current_val-tmp;tmp=mix(min_blend,sub_blend,u_blendMode);}}vec3 color=vec3(1.0-smoothstep(-0.01,0.01,tmp));color*=mix(vec3(0.3,0.7,1.0),vec3(1.0,0.4,0.6),sin(u_time+tmp*10.0)*0.5+0.5);gl_FragColor=vec4(color,1.0);}",
